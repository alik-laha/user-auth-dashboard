{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "src/index.ts"
        },
        {
            "src": "/socket.io/.*",
            "dest": "src/index.ts"
        }
    ],
    "server": {
        "src": "src/index.ts",
        "routes": [
            {
                "src": "/(.*)",
                "dest": "src/index.ts"
            }
        ]
    }
}